local function spin()
local id = idbox.text
local mouse = game.Players.LocalPlayer:GetMouse()
for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
if v.Name == "BoomBox" then
game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
if game.Players.LocalPlayer.Character:FindFirstChild("BoomBox") ~= nil then
game.Players.LocalPlayer.Character.BoomBox.Remote:FireServer("PlaySong", id)
wait(0.1)
game.Players.LocalPlayer.Character.BoomBox.Server:Destroy()
game.Players.LocalPlayer.Character.Humanoid:UnequipTools(v)
end
end
end
for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
if v.Name == "BoomBox" then
while true do
game.Players.LocalPlayer.Backpack.BoomBox.GripForward = Vector3.new(-0.001, -1, 0.005)
game.Players.LocalPlayer.Backpack.BoomBox.GripPos     = Vector3.new(-1.379, -1.538, -4.341)
game.Players.LocalPlayer.Backpack.BoomBox.GripRight   = Vector3.new(-1, 0.001, -0.014)
game.Players.LocalPlayer.Backpack.BoomBox.GripUp      = Vector3.new(-0.014, 0.005, 1)
game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid:UnequipTools(v)
game.Players.LocalPlayer.Backpack.BoomBox.GripForward = Vector3.new(-0.003, -1, 0.004)
game.Players.LocalPlayer.Backpack.BoomBox.GripPos     = Vector3.new(-1.339, -1.538, -5.056)
