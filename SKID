anim1b.MouseButton1Click:Connect(function()
currentanim.Text = "Current Animation: 1"
end)

anim2b.MouseButton1Click:Connect(function()
currentanim.Text = "Current Animation: 2"
end)


playbutton.MouseButton1Click:Connect(function()
	if currentanim.Text == "Current Animation: 1" then
	anim1()
	IgnoreMe:Destroy()
	spin()
	elseif currentanim.Text == "Current Animation: 2" then
	anim2()
	IgnoreMe:Destroy()
	spin()
	elseif currentanim.Text == "Current Animation: 0" then
	currentanim.Text = "please select a animation"
	wait(1)
	currentanim.Text = "Current Animation: 0"
end
end)
end
end)
